    "express": "^4.18.1",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/inquirer": "^9.0.0",
